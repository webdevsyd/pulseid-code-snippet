{
  "name": "@pulse/code-snippet",
  "version": "1.0.0",
  "description": "Code Snippet",
  "main": "index.js",
  "scripts": {
    "start:frontend": "cross-env NODE_ENV=development webpack serve --config ./webpack.config.js",
    "start:dev-server": "run.env -p ./server/.env-local nodemon ./server/boot.js",
    "start:full-dev": "concurrently --kill-others-on-fail \"npm run start:frontend\"  \"npm run start:dev-server\"",
    "start": "node ./server/boot.js",
    "build": "cross-env NODE_ENV=production webpack --config ./webpack.config.js",
    "predeploy": "npm run copy-statics",
    "copy-statics": "mkdir statics && cp ./dist/* ./statics && rm ./statics/*.map"
  },
